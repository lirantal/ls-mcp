{
  "mcpServers": {
    "pinned-server": {
      "command": "npx",
      "args": ["-y", "mcp-server-nodejs-api-docs@1.1.3"]
    },
    "latest-server": {
      "command": "npx",
      "args": ["-y", "mcp-server-nodejs-api-docs"]
    },
    "docker-server": {
      "command": "docker",
      "args": ["run", "image"]
    },
    "scoped-pinned": {
      "command": "npx",
      "args": ["@upstash/context7-mcp@2.0.0"]
    },
    "scoped-latest": {
      "command": "npx",
      "args": ["-y", "@playwright/mcp"]
    },
    "explicit-latest": {
      "command": "npx",
      "args": ["package@latest"]
    }
  }
}
